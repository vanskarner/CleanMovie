@startuml
!include ../../StylesForDiagrams.puml

left to right direction
title COMPLEMENTARY COMPONENT DIAGRAM
rectangle "domain" {
    component useCases
    component entities
    interface services

    services - useCases
}
rectangle "data" {
    component localData
    component remoteData
}
rectangle presentation {
    component app
}
component core

core <.. app
core <.. useCases
core <.. data

useCases ..> entities
useCases <.. data
entities <.. data
app ..> services
@enduml